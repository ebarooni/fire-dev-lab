    "@eslint/json": "^0.10.0",
    "@eslint/markdown": "^6.3.0",
    "angular-eslint": "^19.2.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-promise": "^7.2.1",
    "firebase-tools": "^13.33.0",
    "prettier": "^3.5.3",
    "swiftlint": "^2.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.0"
